    "express-validator": "^6.4.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.13.20"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.20",
    "react-uuid": "^1.0.2"
  }
}
